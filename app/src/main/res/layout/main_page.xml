<?xml version="1.0" encoding="utf-8"?>
<!--디자인3.탭페이지(drawer버튼)-->
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TabHost
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@android:id/tabhost">
            <LinearLayout
                android:id="@+id/core_linear_layout"
                android:orientation="vertical"
                android:layout_height="match_parent"
                android:layout_width="match_parent">
                <!-- 메뉴 하나하나 시작-->
                <TabWidget
                    android:id="@android:id/tabs"
                    android:layout_height="50dp"
                    android:tabStripEnabled="false"
                    android:tabStripLeft="@drawable/tab_under_line"
                    android:tabStripRight="@drawable/tab_under_line"
                    style="@style/tabTheme"
                    android:layout_width="match_parent"/>
                <!-- 메뉴 하나하나 끝-->
                <!--본문 시작-->
                <FrameLayout
                    android:id="@android:id/tabcontent"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"/>
                <!--본문 끝-->
            </LinearLayout>
        </TabHost>
    </LinearLayout>
    <!--여기 width-->
    <SlidingDrawer
        android:id="@+id/drawer_left"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:handle="@+id/handle_left"
        android:content="@+id/content_left"
        android:gravity="center_horizontal"
        android:rotation="180"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/handle_left"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:src="@drawable/national_matching_drawer_button" />
        <LinearLayout
            android:background="@color/sliding_drawable_background_color"
            android:id="@+id/content_left"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"><!--레이아웃 시작-->
            <TextView
                android:textColor="@color/color_while"
                android:id="@+id/final_oneword"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:gravity="center_horizontal"
                android:textSize="10dp"
                android:rotation="180"
                android:textStyle="bold"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="10dp"
                android:text="*최종 매칭은 한마디 주고 받기 입니다"/>
            <TextView
                android:textColor="@color/color_while"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:gravity="center_horizontal"
                android:textSize="16dp"
                android:rotation="180"
                android:textStyle="bold"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="10dp"
                android:text="- 최종 매칭정보 -"/>
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10">
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_blood"
                    android:layout_weight="6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="조회중"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:textColor="@color/color_while"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:textSize="10dp"
                    android:rotation="180"
                    android:text="혈액형 : "
                    android:fontFamily="sans-serif"/>
                <ImageView
                    android:layout_weight="1"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:rotation="180"
                    android:src="@drawable/nw_page_check_result_img"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10">
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_job"
                    android:layout_weight="6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="조회중"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:textColor="@color/color_while"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:textSize="13dp"
                    android:rotation="180"
                    android:text="직 업 : "
                    android:fontFamily="sans-serif"/>
                <ImageView
                    android:layout_weight="1"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:rotation="180"
                    android:src="@drawable/nw_page_check_result_img"/>
            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10">
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_tall"
                    android:layout_weight="6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="조회중"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:textColor="@color/color_while"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:textSize="13dp"
                    android:rotation="180"
                    android:text=" 키 : "
                    android:fontFamily="sans-serif"/>
                <ImageView
                    android:layout_weight="1"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:rotation="180"
                    android:src="@drawable/nw_page_check_result_img"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal"
                android:weightSum="10">
                <!--사진 시작-->
                <ImageView
                    android:id="@+id/drawer_img"
                    android:layout_width="0dp"
                    android:layout_weight="6"
                    android:layout_height="60dp"
                    android:gravity="center_horizontal"
                    android:rotation="180" />
                <!--사진 끝-->
            </LinearLayout>
            <TextView
                android:textColor="@color/color_while"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:gravity="center_horizontal"
                android:textSize="16dp"
                android:rotation="180"
                android:textStyle="bold"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="10dp"
                android:text="- 2단계 매칭정보 -"/>
            <LinearLayout
                android:textColor="@color/color_while"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10">
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_mind"
                    android:layout_weight="6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="조회중"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:textColor="@color/color_while"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:textSize="13dp"
                    android:rotation="180"
                    android:text="성 격 : "
                    android:fontFamily="sans-serif"/>
                <ImageView
                    android:layout_weight="1"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:rotation="180"
                    android:src="@drawable/nw_page_check_result_img"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10">
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_hobby"
                    android:layout_weight="6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="조회중"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:textColor="@color/color_while"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:textSize="13dp"
                    android:rotation="180"
                    android:text="취 미 : "
                    android:fontFamily="sans-serif"/>
                <ImageView
                    android:layout_weight="1"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:rotation="180"
                    android:src="@drawable/nw_page_check_result_img"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10">
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_smoking"
                    android:layout_weight="6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="조회중"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:textColor="@color/color_while"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:textSize="8dp"
                    android:rotation="180"
                    android:text="흡연여부 : "
                    android:fontFamily="sans-serif"/>
                <ImageView
                    android:layout_weight="1"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:rotation="180"
                    android:src="@drawable/nw_page_check_result_img"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10">
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_local"
                    android:layout_weight="6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="조회중"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:textColor="@color/color_while"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:textSize="13dp"
                    android:rotation="180"
                    android:text="지 역 : "
                    android:fontFamily="sans-serif"/>
                <ImageView
                    android:layout_weight="1"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:rotation="180"
                    android:src="@drawable/nw_page_check_result_img"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10">
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_age"
                    android:layout_weight="6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="조회중"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:textColor="@color/color_while"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:textSize="13dp"
                    android:rotation="180"
                    android:text="나 이 : "
                    android:fontFamily="sans-serif"/>
                <ImageView
                    android:layout_weight="1"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:rotation="180"
                    android:src="@drawable/nw_page_check_result_img"/>
            </LinearLayout>
            <TextView
                android:textColor="@color/color_while"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:gravity="center_horizontal"
                android:textSize="16dp"
                android:rotation="180"
                android:textStyle="bold"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="10dp"
                android:text="- 1단계 매칭정보 -"/>
            <TextView
                android:textColor="@color/color_while"
                android:id="@+id/left_drawer_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:gravity="center_horizontal"
                android:textSize="16dp"
                android:rotation="180"
                android:textStyle="bold"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:text="이름 조회중"/>
            <TextView
                android:textColor="@color/color_while"
                android:id="@+id/left_drawer_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:gravity="center_horizontal"
                android:textSize="12dp"
                android:rotation="180"
                android:textStyle="bold"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:text="이메일 조회중"/>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10">
                <TextView
                    android:textColor="@color/color_while"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="14dp"
                    android:layout_marginTop="-5dp"
                    android:gravity="center_horizontal"
                    android:rotation="180"
                    android:text="호"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_number"
                    android:layout_weight="5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="15dp"
                    android:text="번호"
                    android:rotation="180"
                    android:gravity="center_horizontal"
                    android:textStyle="bold"
                    android:fontFamily="sans-serif" />
                <TextView
                    android:textColor="@color/color_while"
                    android:id="@+id/left_drawer_sex"
                    android:textAlignment="@+id/drawer_number"
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="14dp"
                    android:gravity="center_horizontal"
                    android:layout_marginTop="-5dp"
                    android:text="성별"
                    android:fontFamily="sans-serif"
                    android:rotation="180"/>
            </LinearLayout>
            <!--맨위-->
        </LinearLayout>
    </SlidingDrawer>
</RelativeLayout>